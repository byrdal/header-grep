{% import Toolbar from './toolbar.monk' %}

<Toolbar/>

<table class="pure-table pure-table-striped request-table">
    <tbody>
    {% for requests %}
    <tr>
        <td>
            {% for response.headers %}
            <strong>{{ name }}</strong>: {{ value }}<br/>
            {% endfor %}
        </td>
        <td>{{ request.method }} &nbsp; {{ request.url }}</td>
    </tr>
    {% endfor %}
    </tbody>
</table>
